    "import math\n",
    "\n",
    "def RT(E,V):\n",
    "    k_1=math.sqrt(E)\n",
    "    k_2=math.sqrt((E-V))\n",
    "    R=((k_1-k_2)/(k_1+k_2))**2\n",
    "    T=(4*k_1*k_2)/((k_1+k_2)**2)\n",
    "    print(R,T)\n",
    "    o=R+T\n",
    "    print(o)\n",
    "       \n",
    "RT(10, 9)"
   ]
  }
